class Result {
    public static int findFirstOccurrence(List<Integer> nums, int target) {
     int low=0;
     int high= nums.size()-1;
     int ans=-1;
     while(low<=high ){
        int mid = low+ (high-low)/2;
        if(target==nums.get(mid)){
            ans=mid;
            high=mid-1;
        }
        else if ( target>nums.get(mid))
        {
            low=mid+1;
        }
        else 
        {
            high= mid-1;
        }
        }
    return ans;
        }

}
